<div id="productList">
    <h1>PRODUCTOS POR WEBSOCKET</h1>
    {{#each products}}
    <div id="product-{{this.id}}">
        <p>******************************************</p>
        <p>Title: {{this.title}}</p>
        <p>Description: {{this.description}}</p>
        <p>Category: {{this.category}}</p><p>Code: {{this.code}}</p>
        <p>Price: {{this.price}}</p>
        <p>Stock: {{this.stock}}</p> <p>Status: {{this.status}}</p>
        <p>Id: {{this.id}}</p>
        <p>*****************************************</p>
    </div>
    {{/each}}
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>
